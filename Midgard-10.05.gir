          <doc xml:whitespace="preserve">%TRUE if properties' names are valid, %FALSE otherwise</doc>
          <type name="boolean" c:type="gboolean"/>
        </return-value>
        <parameters>
          <parameter name="property_a" transfer-ownership="none">
            <doc xml:whitespace="preserve">property name</doc>
            <type name="utf8" c:type="gchar*"/>
          </parameter>
          <parameter name="op" transfer-ownership="none">
            <doc xml:whitespace="preserve">comparison operator</doc>
            <type name="utf8" c:type="gchar*"/>
          </parameter>
