      <method name="set_parameter" c:identifier="midgard_object_set_parameter">
        <doc xml:whitespace="preserve">Creates object's parameter object if it doesn't exists, updates otherwise.</doc>
        <return-value transfer-ownership="none">
          <doc xml:whitespace="preserve">%TRUE on success, %FALSE otherwise</doc>
          <type name="boolean" c:type="gboolean"/>
        </return-value>
        <parameters>
